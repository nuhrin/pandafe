    name: Frameskip
    help: Screen update frame skip rate (-1 for auto)
    locked: false
    option: -frameskip
    min-value: -1
    max-value: 100
    default-value: -1
    include-option-for-default-value: false
  - type: double
    name: Frametime
    help: Milliseconds per frame for frameskip auto-adjust (0.0 for default)
    locked: false
    option: ! '-frametime '
