app-id-type: exact
app-id: snes9x.skeezix.alpha
custom-command: ! "#!/bin/bash\n\nARGS=\"$@\"\nROM=${ARGS##* }\nif (( $# > 1 )); then\n\tARGS=${ARGS%
  *}\nelse\n\tARGS=\"\"\nfi\n\necho \"ROM filename is $ROM\"\n\nPWD=`pwd`\necho \"PWD
  pre-run $PWD\"\n\n# set HOME so SRAM saves go to the right place\nHOME=.\nexport
  HOME\n\nexport SDL_VIDEODRIVER=omapdss\nexport SDL_OMAP_VSYNC=1\n\n# run it!\nLD_PRELOAD=./lib/libSDL-1.2.so.0.11.3
  ./snes9x $ARGS $ROM\n#./snes9x $ARGS $ROM"
expected-exit-code: 0
options:
- type: grouping
